// main.jsï¼ˆä¸»çº¿ç¨‹ï¼‰
const myWorker = new Worker('/worker.js'); // åˆ›å»ºworker
myWorker.terminate(); // å…³é—­worker

// worker.jsï¼ˆworkerçº¿ç¨‹ï¼‰ è°ƒç”¨utilså‡½æ•°æ–‡ä»¶

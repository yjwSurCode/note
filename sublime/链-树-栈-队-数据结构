function clone(target, map = new Map()) {
    if (typeof target === 'object') {
        let cloneTarget = Array.isArray(target) ? [] : {}; //è€ƒè™‘åˆ°æ•°ç»„æƒ…å†µäº†
        //è§£å†³å¾ªç¯å¼•ç”¨é—®é¢˜
        if (map.get(target)) {
            return map.get(target);
        }
        map.set(target, cloneTarget);

        for (const key in target) {
            cloneTarget[key] = clone(target[key]);
        }
        console.log('æ‰§è¡Œ')
        return cloneTarget;

    } else {
        return target;
    }
};

æ ˆï¼ˆstackï¼‰åˆåå †æ ˆï¼Œæ˜¯ä¸€ç§éµå¾ªåè¿›å…ˆå‡ºï¼ˆLIFOï¼‰åŸåˆ™çš„æœ‰åºé›†åˆ,æ–°æ·»åŠ æˆ–å¾…åˆ é™¤çš„å…ƒç´ éƒ½ä¿å­˜åœ¨æ ˆçš„æœ«å°¾ï¼Œç§°ä½œæ ˆé¡¶ï¼Œå¦ä¸€ç«¯ç§°ä½œæ ˆåº•


function Stack() {
  this.dataStore = []; //åˆå§‹åŒ–ä¸ºç©º
  this.top = 0; //è®°å½•æ ˆé¡¶ä½ç½®
  this.pop = pop; //å‡ºæ ˆ
  this.push = push; //å…¥æ ˆ
  this.peek = peek; //æŸ¥çœ‹æ ˆé¡¶å…ƒç´ 
  this.length = length; //æŸ¥çœ‹æ ˆå†…å…ƒç´ æ€»æ•°
  this.clear = clear; //æ¸…ç©ºæ ˆ

  function push(element) {
    this.dataStore[this.top++] = element;
  }

  //å–å‡ºæ ˆé¡¶å…ƒç´ 
  function pop() {
    return this.dataStore[--this.top];
  }

  function peek() {
    if (this.top > 0) return this.dataStore[this.top - 1];
    else return "Empty";
  }

  function clear() {
    delete this.dataStore;
    this.dataStore = [];
    this.top = 0;
  }

  function length() {
    return this.top;
  }
}

var stack = new Stack();
console.log(stack.peek());
stack.push("111");
console.log(stack.peek());

å›æ–‡åˆ¤æ–­ï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼šï¼š
function isPalindrome(word) {
  var s = new Stack();
  for (var i = 0; i < word.length; i++) {
    s.push(word[i]);
  }

  var rword = "";

  while (s.length() > 0) {
    // rword += s.pop();
    rword = s.pop() + rword;
  }

  if (word === rword) {
    return true;
  } else {
    return false;
  }
}
console.log(isPalindrome("level")); // true


